{
	"name": "Week6_Review_Pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy_house-price_data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSetting",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSetting"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSetting"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSetting",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DelimitedText12",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText13",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Read_data_and_predict_price",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Copy_house-price_data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/gayatri.jadhav1@cgm4l.onmicrosoft.com/Week6_Review_File",
					"baseParameters": {
						"Input_file": {
							"value": "@pipeline().parameters.Input_file",
							"type": "Expression"
						},
						"File_name": {
							"value": "@pipeline().parameters.File_name",
							"type": "Expression"
						},
						"Output_file": {
							"value": "@pipeline().parameters.Output_file",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "gayatriDatabricks_LinkedService",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"Input_file": {
				"type": "string",
				"defaultValue": "/Input_file"
			},
			"File_name": {
				"type": "string",
				"defaultValue": "house-prices.csv"
			},
			"Output_file": {
				"type": "string",
				"defaultValue": "Output_file"
			}
		},
		"annotations": []
	}
}